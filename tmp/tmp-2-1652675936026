<script defer>
	$(document).ready(function(){
		var $proContent = $('#product .short-description');
		if($proContent.height() > 140){
			$proContent.css("height","140px");
			$proContent.css("overflow","hidden");
			$('.ega-pro__seemore').click(function(){
				$(this).toggleClass("show");
				if($(this).hasClass('show')) {
					$(this).find('a').html('Thu gọn')
					$proContent.css("height","auto");
				}else {
					$proContent.css("height","140px");
					$(this).find('a').html('Xem thêm')
				}
			})
		}else{
			$('.ega-pro__seemore1').addClass("hidden");
		}
	if($(".swatch-element").text().includes('tặng'))
	{
		$('.select-swatch').hide()
	}
	setTimeout(function(){
		if($("#pro_sku").text().includes('tặng'))
		{
			$('#pro_sku,.pro-price').hide();
			$("#add-to-cart").attr('disabled','').text('Hết hàng')
			$("#add-to-cartbottom").attr('disabled','').text('Hết hàng')
			$(".btnBuyNow").attr('disabled','').text('Hết hàng')
		}
	},1000)
	$('#add-to-cart').click(function(e){	
		e.preventDefault();
		$(this).addClass('clicked_buy');
		add_item_show_modalCart($('#product-select').val());
		getCartModal();
	});
	$('#add-to-cartbottom').click(function(){	
		$('#add-to-cart').trigger('click');
	});
	$('#quan-input').keyup(function(){
		$('[name="quantity"]').val($(this).val());
	})
	$('[name="quantity"]').on('keyup change', function(){
		$('#quan-input').val($(this).val());
	})
	$(document).on("click","#product-zoom-in",function(){
		//	var indexThumb = $(this).index();
		$("body").addClass("open_layer");
		$("#divzoom").css({'opcaity':0,'visibility':'hidden'}).show();
		$('.divzoom_main').flickity({
			resize:true,
			draggable: true,
		});
		if($(window).width() > 768){
			var ncurrent = parseInt($(".gallery-index .current").html()) - 1;
		}
		else{
			var ncurrent = parseInt($(".product-gallery-item.is-selected").index());
		}
		$('.divzoom_main').flickity('select',ncurrent);
		setTimeout(function(){$("#divzoom").css({'opcaity': 1,'visibility':'visible'})},50);
	});
	$(document).on('click','#closedivZoom', function(event) {
		$("#divzoom").hide();
		$("body").removeClass("open_layer");
		$('.divzoom_main').flickity('select',0);
		//$('.divzoom_main').slick('unslick');
	});
	});
</script>
